; a)


(define (length2 items)
  (define (length_count items y)
    (if (null? items)
        y
        (length_count (cdr items)
                      (+ y 1))))
  (length_count items 0))



; b)


; Den eneste måten jeg fikk prosedyren til å virke var å lage en en hjelpe prosedyre som bruker halerekursjon
; Dette ga opphav til en iterativ prosess ettersom den bare returnerer akkumulatorvariblen når den har nådd basistilfellet

(define (reduce-reverse proc init items)
  (define (reverse in out)
    (if (null? in)
        out
        (reverse (cdr in)
                 (cons (car in) out))))
  (reverse items init))


; c)

(define (all? proc items)
  (if #f (proc (car items)))
      #f
      (proc (car (cdr items))))


         
  
         
  


  










